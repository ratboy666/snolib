unix2dos -n SYSTEM.INC SYSTEM.SPT
unix2dos -n SDIFF.INC SDIFF.SPT
unix2dos -n SWAP.INC SWAP.SPT
cat BRKREM.INC | sed -e "s/\.INC'/.SPT'/" >BRKREM.SPT
unix2dos BRKREM.SPT
unix2dos -n CRACK.INC CRACK.SPT
unix2dos -n COUNT.INC COUNT.SPT
unix2dos -n SEQ.INC SEQ.SPT
unix2dos -n AGT.INC AGT.SPT
cat SIZEA.INC | sed -e "s/\.INC'/.SPT'/" >SIZEA.SPT
unix2dos SIZEA.SPT
cat AI.INC | sed -e "s/\.INC'/.SPT'/" >AI.SPT
unix2dos AI.SPT
cat AOPA.INC | sed -e "s/\.INC'/.SPT'/" >AOPA.SPT
unix2dos AOPA.SPT
unix2dos -n BALREV.INC BALREV.SPT
unix2dos -n REPL.INC REPL.SPT
cat UNIQUE.INC | sed -e "s/\.INC'/.SPT'/" >UNIQUE.SPT
unix2dos UNIQUE.SPT
unix2dos -n LINK.INC LINK.SPT
cat BALX.INC | sed -e "s/\.INC'/.SPT'/" >BALX.SPT
unix2dos BALX.SPT
